import 'package:flutter/material.dart';
import 'package:get/get.dart';

void main() {
  runApp(GetMaterialApp(
    home: Home(),
  ));
}

class Home extends StatefulWidget {
  Home({Key? key}) : super(key: key);

  @override
  State<Home> createState() => _HomeState();
}

RxInt count = 0.obs;
student controller = Get.put(student());

class _HomeState extends State<Home> {
  @override
  Widget build(BuildContext context) {
    return Scaffold(
      body: Column(
        crossAxisAlignment: CrossAxisAlignment.center,
        mainAxisAlignment: MainAxisAlignment.center,
        children: [
          Image.network(
              "data:image/jpeg;base64,"),
          Center(
            child: GestureDetector(
                onTap: () {
                  controller.converttouppercase();
                  student.kishan.value = student.kishan.toUpperCase();
                },
                child: Obx(() => Text("${student.kishan}${student.vinas}"))),
          ),
        ],
      ),
    );
  }
}

class student extends GetxController {
  static var vinas = 0.obs;
  static RxString kishan = "fvfd".obs;

  void converttouppercase() {
    vinas.value = vinas.value + 1;
  }
}
