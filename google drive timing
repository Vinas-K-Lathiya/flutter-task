final fileUrl = 'https://docs.google.com/document/d/19GnuL_xvgmx2Jh26HkOG7e7r5z_Ya8QpIuY0PoJa2Lw/edit?usp=drive_link';


        final response = await http.head(Uri.parse(fileUrl));

        if (response.statusCode == 200) {
          print(response.statusCode);
          print(response.body);
           String lastModifiedHeader = response.headers['date'] ?? "";
          DateTime originalDate = DateFormat('E, d MMM y HH:mm:ss Z').parseUtc(lastModifiedHeader).toLocal();

          // Format the date according to "dd-MM-yyyy HH:mm:ss"
          String formattedDate = DateFormat('dd-MM-yyyy hh:mm:ss a').format(originalDate);
          print(formattedDate);
        }
